@model IReadOnlyCollection<Course>

@foreach (var course in Model)
{
    //jako tabela
    <h1>@course.Name</h1>
    <p>@course.Description</p>
    <p>@course.Deprecated</p>
    if (User.IsInRole("Student"))
    {
        <a asp-controller="Course" asp-action="Enroll" asp-route-courseId="@course.CourseId">Enroll</a>
    }
}